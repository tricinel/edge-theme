{
  "alfredtheme" : {
    "result" : {
      "textSpacing" : 8,
      "subtext" : {
        "size" : 11,
        "colorSelected" : "{{ toHex8(colors.text.contrast.high) }}",
        "font" : "System Light",
        "color" : "{{ toHex8(colors.text.contrast.light) }}"
      },
      "shortcut" : {
        "size" : 12,
        "colorSelected" : "{{ toHex8(colors.text.caret) }}",
        "font" : "System Light",
        "color" : "{{ toHex8(colors.text.accents.white) }}"
      },
      "backgroundSelected" : "{{ toHex8(colors.backgrounds.highlight) }}",
      "text" : {
        "size" : 16,
        "colorSelected" : "{{ toHex8(colors.text.caret) }}",
        "font" : "System Light",
        "color" : "{{ toHex8(colors.text.accents.white) }}"
      },
      "iconPaddingHorizontal" : 4,
      "paddingVertical" : 8,
      "iconSize" : 24
    },
    "search" : {
