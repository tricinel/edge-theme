    "credit" : "Bogdan Lazar",
    "separator" : {
      "color" : "{{ toHex8(colors.backgrounds.background) }}",
      "thickness" : 1
    },
    "scrollbar" : {
      "color" : "{{ toHex8(colors.backgrounds.foreground) }}",
      "thickness" : 2
    },
    "name" : "{{ name }}"
  }
